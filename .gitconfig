[core]
  editor = vim
  autocrlf = input
[diff]
  tool = vimdiff
[color]
  ui = auto
  branch = auto
  interactive = auto
  status = auto
[alias]
  st = status
  stat = status
  sb = submodule
  co = checkout
  br = branch
  cp = cherry-pick
[push]
  default = current
[merge]
  ff = no
[credential]
  helper = store
[include]
  path = ~/.custom.gitconfig
